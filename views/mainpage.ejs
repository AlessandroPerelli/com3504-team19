<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script
      src="https://kit.fontawesome.com/8adee5f06e.js"
      crossorigin="anonymous"
    ></script>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Plus+Jakarta+Sans:ital,wght@0,200..800;1,200..800&display=swap"
      rel="stylesheet"
    />
    <link rel="stylesheet" href="/stylesheets/main.css" />
    <title>Main</title>
  </head>
  <body>
    <% include components/navbar.ejs %>
    <div class="main-page-container flex">
      <h1>Explore plants</h1>
      <i class="fa-solid fa-filter"></i>
      <div class="category-links">
        <% categoryData.forEach(function(category) { %> <% var categoryName =
        category.categoryName; %> <% include components/categoryfilter.ejs %> <%
        }); %>
      </div>
      <% categoryData.forEach(function(category) { %> <% var categoryName =
      category.categoryName; %> <% var plants = category.plants; %> <% include
      components/category.ejs %> <% }); %>
    </div>

    <div id="addPlantOverlay" class="overlay">
      <div class="overlay-content">

      </div>
    </div>

    <div id="overlay" class="overlay">
      <div class="overlay-content">

      </div>
    </div>

    <button id="addPlantButton" class="add-plant-button">Add Plant</button>

    <script src="/javascripts/overlay.js"></script>
  </body>
</html>
