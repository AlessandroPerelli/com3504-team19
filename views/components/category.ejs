<div class="category">
  <h3><%= categoryName %></h3>
  <div class="plant-grid">
    <% plants.forEach(plant => { %>
      <div class="plant-item" data-plant-id="<%= plant.id %>" data-category="<%= categoryName %>">
        <img src="<%= plant.image %>" alt="plant" />
      </div>
    <% }) %>
  </div>
</div>