<div class="category">
  <h3><%= categoryName %></h3>
  <div class="plant-grid">
    <% plants.forEach(plant => { %>
      <a href="/viewplant?id=<%= plant.id %>">
        <div class="plant-item">
          <img src="<%= plant.image %>" alt="plant" />
        </div>
      </a>
    <% }) %>
  </div>
</div>
