<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Update Plant Name</title>
</head>
<body>
<h1>Update Plant Name</h1>
<form id="updatePlantForm">
    <label for="plantId">Plant ID:</label>
    <input type="text" id="plantId" name="plantId" required>
    <br>
    <label for="newPlantName">New Plant Name:</label>
    <input type="text" id="newPlantName" name="newPlantName" required>
    <br>
    <button type="submit">Update Plant Name</button>
</form>

<script>
    document.getElementById('updatePlantForm').addEventListener('submit', function(event) {
        event.preventDefault();
        const plantId = document.getElementById('plantId').value;
        const newPlantName = document.getElementById('newPlantName').value;

        console.log(plantId)
        console.log(newPlantName)
        newPlantName.updateName(plantId,newPlantName)
    });
</script>
</body>
</html>
